{
  "dart.flutterSdkPath": ".fvm/versions/3.24.0",
  "dart.lineLength": 120,
  "editor.rulers": [
    120
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always",
    "source.fixAll": "always"
  },
  "files.insertFinalNewline": true,
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.fvm": true,
  },
  "search.exclude": {
    "**/.fvm": true,
  },
}
